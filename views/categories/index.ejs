<% layout('./layouts/boilerplate') %>
  <h1 style="text-align: center; margin-bottom: 50px">Welcome To All Categories Page</h1>

  <div class="col-6 offset-3 card-links" style="margin-bottom: 100px;">
    <ul class="list-group">
      <% for (c of categories) {%>
        <li class="list-group-item d-flex justify-content-between align-items-center">
          <a class= "labels"id="<%= c %>"href="/categories/<%= c %>">
            <%= c %>
          </a><span class="data"> <%= counts.get(c) %></span>
        </li>
      <% } %>
    </ul>
  </div>
  <div class="col-6  offset-3 category-chart">
    <canvas id="myChart2">sfgsfdg</canvas>
  </div>

  